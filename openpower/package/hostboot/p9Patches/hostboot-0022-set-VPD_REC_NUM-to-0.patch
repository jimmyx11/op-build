     my ($num,$base,$group_offset,$proc_offset,$offset) = split(/,/,
          $targetObj->getAttribute($target,"XSCOM_BASE_ADDRESS"));
 
-    $targetObj->setAttribute($target, "XSCOM_BASE_ADDRESS", $base);
